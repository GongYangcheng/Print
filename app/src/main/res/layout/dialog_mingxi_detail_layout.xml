<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/free_dialog_bg"
    android:orientation="vertical">

    <include layout="@layout/dialog_toolbar_layout"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp10"
        android:layout_marginVertical="@dimen/dp10"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/tv_bill_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="单号："
            android:textColor="@color/whiteFFF"
            android:textSize="@dimen/sp20"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="9255282561"
            android:textColor="@color/whiteFFF"
            android:textSize="@dimen/sp20"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp10"
        android:layout_marginVertical="@dimen/dp10"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="品名："
            android:textColor="@color/whiteFFF"
            android:textSize="@dimen/sp20"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="插头插座MSSD-EB-M12"
            android:textColor="@color/whiteFFF"
            android:textSize="@dimen/sp20"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp10"
        android:layout_marginVertical="@dimen/dp10"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="库位："
            android:textColor="@color/whiteFFF"
            android:textSize="@dimen/sp20"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="14-62-15-3"
            android:textColor="@color/whiteFFF"
            android:textSize="@dimen/sp20"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp10"
        android:layout_marginVertical="@dimen/dp10"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="SKU："
            android:textColor="@color/whiteFFF"
            android:textSize="@dimen/sp20"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="539712"
            android:textColor="@color/whiteFFF"
            android:textSize="@dimen/sp20"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp10"
        android:layout_marginVertical="@dimen/dp10"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="批次号："
            android:textColor="@color/whiteFFF"
            android:textSize="@dimen/sp20"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="无"
            android:textColor="@color/whiteFFF"
            android:textSize="@dimen/sp20"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp10"
        android:layout_marginVertical="@dimen/dp10"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="单位："
            android:textColor="@color/whiteFFF"
            android:textSize="@dimen/sp20"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="箱"
            android:textColor="@color/whiteFFF"
            android:textSize="@dimen/sp20"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginVertical="@dimen/dp10"
        android:layout_marginLeft="@dimen/dp10"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="拣选数量："
            android:textColor="@color/whiteFFF"
            android:textSize="@dimen/sp20"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="12"
            android:textColor="@color/whiteFFF"
            android:textSize="@dimen/sp20"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp10"
        android:layout_marginVertical="@dimen/dp10"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="已拣数量："
            android:textColor="@color/whiteFFF"
            android:textSize="@dimen/sp20"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:textColor="@color/whiteFFF"
            android:textSize="@dimen/sp20"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginVertical="@dimen/dp10"
        android:layout_marginLeft="@dimen/dp10"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="箱号："
            android:textColor="@color/whiteFFF"
            android:textSize="@dimen/sp20"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="无"
            android:textColor="@color/whiteFFF"
            android:textSize="@dimen/sp20"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp10"
        android:layout_marginVertical="@dimen/dp10"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="状态："
            android:textColor="@color/whiteFFF"
            android:textSize="@dimen/sp20"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="拣选中"
            android:textColor="@color/whiteFFF"
            android:textSize="@dimen/sp20"/>
    </LinearLayout>

</LinearLayout>
